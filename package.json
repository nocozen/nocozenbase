{
  "name": "nocozenbase",
  "version": "v1.2.0",
  "description": "No-code software building platform server",
  "long_description": "built with Node.js, TypeScript, and HyperExpress.",
  "main": "index.js",
  "scripts": {
    "pmrun": "pm2 start ecosystem.config.js",
    "pmdel": "pm2 delete all",
    "dev": "npx dotenvx run -f .env -- tsx watch ./src/server.ts",
    "jsrun": "node ./dist/server.js",
    "tsrun": "npx tsx ./src/server.ts",
    "tw": "tsc --watch",
    "ts": "tsc",
    "nccdev": "ncc build src/server.ts -o dist",
    "nccprod": "ncc build src/server.ts -o dist --no-source-map-register --no-cache --minify",
    "build": "npx tsup src/server.ts"
  },
  "keywords": [
    "node",
    "typescript",
    "hyper-express",
    "no-code"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "author": {
    "name": "qianbone",
    "email": "qb@qianbone.com",
    "url": "https://qianbone.com"
  },
  "license": "AGPLv3",
  "homepage": "https://qianbone.com",
  "repository": {
    "url": "https://qianbone.com"
  },
  "bugs": {
    "url": "https://qianbone.com"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/ua-parser-js": "^0.7.39",
    "commander": "^14.0.0",
    "pm2": "^5.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.45.1",
    "@msgpack/msgpack": "^3.1.2",
    "@noble/ciphers": "^1.3.0",
    "@noble/curves": "^1.9.4",
    "@pulsecron/pulse": "^1.6.8",
    "bcryptjs": "^3.0.2",
    "conf": "^14.0.0",
    "dayjs": "^1.11.13",
    "hyper-express": "^6.17.3",
    "jose": "^6.0.11",
    "live-directory": "^3.0.3",
    "lru-cache": "^11.1.0",
    "map-factory": "^3.8.7",
    "mongodb": "^7.0.0",
    "ms": "^2.1.3",
    "msgpackr": "^1.11.4",
    "node-object-hash": "^3.1.1",
    "pino": "^9.7.0",
    "radashi": "^12.5.0",
    "reflect-metadata": "^0.1.14",
    "rotating-file-stream": "^3.2.6",
    "rulepilot": "^1.4.9",
    "tsx": "^4.20.3",
    "ua-parser-js": "^2.0.4"
  },
  "website": "https://qianbone.com"
}
